<%-include('../../partials/header.ejs')%> 
<body>
    <%-include('../../partials/navbarAdmin.ejs')%> 
    <div class="container">
        <br>
        <h2>Users</h2>
        <div style="text-align: right;">
            <a class="btn btn-success" href="/admin/users/create" >Novo Usuario</a> 
        </div>
        <br>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">Id</th>
                <th scope="col">Email</th>
              </tr>
            </thead>
            <tbody>
              <% users.forEach(user => { %> 
                <tr>
                  <th scope="row"><%=user.id %></th>
                  <td><%=user.email%></td>
                </tr>
              <% }) %> 
            </tbody>
          </table>
    </div>
    
</body>


<%-include ('../../partials/footer.ejs')%> 

